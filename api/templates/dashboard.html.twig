{% block body %}

<div class="panel panel-default">
  <div class="panel-heading">Assets</div>
  <div class="panel-body">
    <ul>
      <li>Assets: {{ stats.countAssets }}
    </li>
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-heading">Instances</div>
